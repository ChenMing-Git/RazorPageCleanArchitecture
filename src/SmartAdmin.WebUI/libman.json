{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "axios@0.21.1",
      "destination": "wwwroot/lib/axios/",
      "files": [
        "dist/axios.js",
        "dist/axios.map",
        "dist/axios.min.js",
        "dist/axios.min.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "numeral.js@2.0.6",
      "destination": "wwwroot/lib/numeral/"
    },
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}
