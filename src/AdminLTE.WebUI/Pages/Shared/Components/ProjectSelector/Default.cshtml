@model AdminLTE.WebUI.Pages.Shared.Components.ProjectSelector.DefaultModel
<div class="dropdown-menu">
        @foreach (var item in Model.Projects)
        {
           <form method="GET" >
             <input type="hidden" name="projectName" value="@item.Name" />
             <input type="hidden" name="projectId" value="@item.Id" />
             <a href="javascript:void()" onclick="this.parentElement.submit();" class="dropdown-item" >@item.Name</a>
           </form>
        }
</div>

